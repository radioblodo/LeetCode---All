class Solution:
    def relativeSortArray(self, arr1: List[int], arr2: List[int]) -> List[int]:
        numbersNotInArr2 = []
        for num in arr1:
            if num not in arr2:
                numbersNotInArr2.append(num)

        numbersNotInArr2 = sorted(numbersNotInArr2)

        ans = []
        for num in arr2:
            for i in range(arr1.count(num)):
                ans.append(num)
            
        return ans + numbersNotInArr2
        

        
